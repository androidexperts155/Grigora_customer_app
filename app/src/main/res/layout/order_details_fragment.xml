    <androidx.coordinatorlayout.widget.CoordinatorLayout
        android:id="@+id/parentView"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        
        <fragment
            android:id="@+id/mMap"
            android:name="com.google.android.gms.maps.SupportMapFragment"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />
        
        <ImageView
            android:id="@+id/transparentImage"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentTop="true"
            android:src="@android:color/transparent" />
        
        
        <RelativeLayout
            android:id="@+id/li_bottomsheet"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingTop="@dimen/_20sdp"
            app:behavior_hideable="true"
            app:layout_behavior="com.google.android.material.bottomsheet.BottomSheetBehavior">
            
            
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="@dimen/_60sdp"
                android:background="#fefefe"
                android:orientation="vertical"
                
                android:paddingTop="8dp"
                android:paddingBottom="@dimen/_16sdp">
            
            
            </LinearLayout>
            
            
            <RelativeLayout
                android:id="@+id/rel_card"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_16sdp"
                android:layout_marginTop="8dp"
                android:layout_marginRight="@dimen/_16sdp"
                android:background="@drawable/shadow"
                android:padding="16dp">
                
                <TextView
                    
                    android:id="@+id/tv_rest_name"
                    style="@style/normalText"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginTop="8dp"
                    android:textColor="@color/textBlack"
                    android:textSize="@dimen/_10sdp"
                    tools:text="Restaurant name" />
                
                <TextView
                    android:id="@+id/tv_order_id"
                    style="@style/normalText"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/tv_rest_name"
                    android:layout_alignParentLeft="true"
                    android:layout_marginTop="8dp"
                    android:textColor="@color/textBlack"
                    android:textSize="@dimen/_10sdp"
                    tools:text="@string/order_hash" />
                
                <TextView
                    android:id="@+id/tv_est_delivery"
                    style="@style/normalText"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/tv_order_id"
                    android:layout_alignParentLeft="true"
                    android:layout_marginTop="8dp"
                    android:layout_marginEnd="8dp"
                    android:layout_toStartOf="@+id/bt_cancel"
                    android:layout_toRightOf="@+id/bt_cancel"
                    android:maxLines="2"
                    android:textColor="@color/textBlack"
                    android:textSize="@dimen/_10sdp" />
                
                <Button
                    android:id="@+id/bt_cancel"
                    style="@style/normalText"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/_25sdp"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginTop="8dp"
                    android:layout_marginEnd="8dp"
                    android:layout_marginBottom="8dp"
                    android:background="@color/colorPrimaryDark"
                    android:text="@string/places_cancel"
                    
                    android:textAllCaps="false"
                    android:textColor="@color/white"
                    android:textSize="@dimen/_10sdp"
                    android:visibility="gone" />
                
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/tv_est_delivery"
                    android:layout_marginTop="8dp">
                    
                    <com.github.siyamed.shapeimageview.CircularImageView
                        android:id="@+id/img_driver"
                        android:layout_width="@dimen/_28sdp"
                        android:layout_height="@dimen/_28sdp"
                        android:layout_alignParentLeft="true"
                        app:srcCompat="@drawable/ic_user" />
                    
                    <TextView
                        android:id="@+id/tv_driver_name"
                        style="@style/normalText"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginStart="8dp"
                        android:layout_marginEnd="8dp"
                        android:layout_toStartOf="@+id/img_call"
                        android:layout_toEndOf="@+id/img_driver"
                        android:layout_toRightOf="@+id/img_driver"
                        android:maxLines="2"
                        android:textColor="@color/textBlack"
                        android:textSize="@dimen/_10sdp"
                        tools:text="Driver Name" />
                    
                    <ImageView
                        android:id="@+id/img_call"
                        android:layout_width="@dimen/_18sdp"
                        android:layout_height="@dimen/_18sdp"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginEnd="8dp"
                        app:srcCompat="@drawable/ic_telephone" />
                </RelativeLayout>
            </RelativeLayout>
            
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_below="@+id/rel_card"
                android:orientation="vertical">
                
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_8sdp"
                    android:orientation="vertical"
                    android:paddingLeft="@dimen/_16sdp">
                    
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center_vertical"
                        android:orientation="horizontal">
                        
                        <ImageView
                            android:id="@+id/img_1"
                            android:layout_width="@dimen/_11sdp"
                            android:layout_height="@dimen/_11sdp"
                            app:srcCompat="@drawable/ic_circular_shape_grey" />
                        
                        <TextView
                            android:id="@+id/tv_1"
                            style="@style/normalText"
                            android:layout_width="wrap_content"
